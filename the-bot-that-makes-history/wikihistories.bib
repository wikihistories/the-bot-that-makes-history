	keywords = {memory, media, Holocaust, bystander, complicity, digital, video games},
	pages = {134--151},
	file = {Full Text PDF:/Users/michaelgfalk/Zotero/storage/BEQGSN3P/Glouftsis - 2022 - Implicated Gaming Choice and Complicity in Ludic .pdf:application/pdf},
