	keywords = {technology, time, Anthropocene, temporality, anticipatory practices, historical futures, modalities of the future, technoscience, transition},
	pages = {3--22},
	file = {Full Text PDF:/Users/michaelgfalk/Zotero/storage/7T4UJPF2/Simon and Tamm - 2021 - Historical Futures.pdf:application/pdf},
}

